  <sourceMessage kind="0">
   <name>No_Message</name>
   <recordMapping>
    <fullname>DEFAULT#1</fullname>
    <textline>// define variables, set counter
</textline>
    <textline>local size%, i%, volume%;
</textline>
    <textline>volume% = 1;
</textline>
    <textline>
</textline>
    <textline>// define array (first index is always 0)
</textline>
    <textline>// it's not possible to use a variable for the size
</textline>
    <textline>local harrypotter$[7][2]; 
</textline>
    <textline>
</textline>
    <textline>// fill the array
</textline>
    <textline>// first token is the volume [0]-[6]
</textline>
    <textline>// second token is the name of the book [0] and the ISBN [1]
</textline>
    <textline>harrypotter$[0][0] = "Harry Potter and the Philosopher's Stone";
</textline>
    <textline>harrypotter$[0][1] = "ISBN 0-7475-7360-3";
</textline>
    <textline>harrypotter$[1][0] = "Harry Potter and the Chamber of Secrets";
</textline>
    <textline>harrypotter$[1][1] = "ISBN 0-7475-3849-2";
</textline>
    <textline>harrypotter$[2][0] = "Harry Potter and the Prisoner of Azkaban";
</textline>
    <textline>harrypotter$[2][1] = "ISBN 0-7475-4215-5";
</textline>
    <textline>harrypotter$[3][0] = "Harry Potter and the Goblet of Fire";
</textline>
    <textline>harrypotter$[3][1] = "ISBN 0-7475-4624-X";
</textline>
    <textline>harrypotter$[3][2] = "CD";
</textline>
    <textline>harrypotter$[3][3] = "Music";
</textline>
    <textline>harrypotter$[4][0] = "Harry Potter and the Order of the Phoenix";
</textline>
    <textline>harrypotter$[4][1] = "ISBN 0-7475-5100-6";
</textline>
    <textline>harrypotter$[5][0] = "Harry Potter and the Half-Blood Prince";
</textline>
    <textline>harrypotter$[5][1] = "ISBN 0-7475-8108-8";
</textline>
    <textline>harrypotter$[6][0] = "Harry Potter and the Deathly Hallows";
</textline>
    <textline>harrypotter$[6][1] = "ISBN 0-7475-9106-7";
</textline>
    <textline>
</textline>
    <textline>//get array size for defining how often the for loop must run 
</textline>
    <textline>size% = getSize(harrypotter$[]) - 1; //The getSize function's output is 7, but as the index starts with 0, the highest index is 6!
</textline>
    <textline>traceln("Max array index: " &amp; size%);
</textline>
    <textline>
</textline>
    <textline>// get array data
</textline>
    <textline>for i% = 0 to size% // Loop effecting the read out of the elements of a two-dimensional
</textline>
    <textline>    traceln("Harry Potter Volume "&amp; volume% &amp; " is \"" &amp; harrypotter$[i%][0]&amp; "\" with " &amp; harrypotter$[i%][1] &amp; "."); 
</textline>
    <textline>    volume% = volume% + 1;
</textline>
    <textline>next
</textline>
    <textline>
</textline>
    <textline>// Excercise 3 - some additional info
</textline>
    <textline>
</textline>
    <textline>// The size of the first dimension
</textline>
    <textline>traceln("getsize(harrypotter$[]): " &amp;getsize(harrypotter$[]));            
</textline>
    <textline>// The size of the second dimension of the 3rd occurence of the first dimension (0-based)
</textline>
    <textline>traceln("getsize(harrypotter$[2]): " &amp;getsize(harrypotter$[2]));          
</textline>
    <textline>// The size of the second dimension of the 4th occurence of the first dimension (0-based)
</textline>
    <textline>traceln("getsize(harrypotter$[3]): " &amp;getsize(harrypotter$[3]));          
</textline>
    <textline>// The size of the value contained in this matrix
</textline>
    <textline>traceln("getsize(harrypotter$[6][0]): " &amp;getsize(harrypotter$[6][0]));    
</textline>
    <textline/>
