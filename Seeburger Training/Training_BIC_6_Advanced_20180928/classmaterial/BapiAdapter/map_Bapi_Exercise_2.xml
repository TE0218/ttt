    <textline>
</textline>
    <textline>if kunag$ &amp;!= kunwe$
</textline>
    <textline>    addBAPITableRow();
</textline>
    <textline>    setBAPITableFieldValue("PARTN_ROLE", "WE");
</textline>
    <textline>    setBAPITableFieldValue("PARTN_NUMB", kunwe$);
</textline>
    <textline>endif  
</textline>
    <textline>  
</textline>
    <textline>// set Items
</textline>
