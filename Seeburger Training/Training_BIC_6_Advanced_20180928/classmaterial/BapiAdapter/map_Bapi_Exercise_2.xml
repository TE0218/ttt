    <textline>item_quantity$ = "5";
</textline>
    <textline>unit$ = "ST";
</textline>
    <textline>sched_line$ = "10";
</textline>
    <textline>date$ = "20131211";
</textline>
    <textline>sched_quantity$ = "5";
</textline>
    <textline>
</textline>
    <textline>getBAPIImportStructure("WA_ORDER_HEADER_IN");
