</textline>
    <textline>setBAPITableFieldValue("NET_WEIGHT", "3");
</textline>
    <textline>setBAPITableFieldValue("UNTOF_WGHT", "G");
</textline>
    <textline>
</textline>
    <textline>// set Schedules
</textline>
    <textline>getBapiTable("WA_ORDER_SCHEDULES_IN");
</textline>
    <textline>addBapiTableRow();
</textline>
    <textline>setBapiTableFieldValue("ITM_NUMBER", item_number$);
</textline>
    <textline>setBapiTableFieldValue("SCHED_LINE", sched_line$);
</textline>
    <textline>setBapiTableFieldValue("REQ_DATE", date$);
</textline>
