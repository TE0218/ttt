</textline>
    <textline>setBapiTableFieldValue("LOAD_DATE", date$);
</textline>
    <textline>setBapiTableFieldValue("GI_DATE", date$);
</textline>
    <textline>
</textline>
    <textline>traceln("Create Sales Order");
</textline>
    <textline>executeBapiCall();
</textline>
    <textline>
</textline>
    <textline>ordernumber$ = getBapiExportValue("WA_SALESDOCUMENT");
</textline>
    <textline>traceln("Order number: "&amp;ordernumber$);
</textline>
    <textline>
</textline>
